    font-style: normal;
    
}
.nav-wrapper a {
    text-align: right;
    right:0;
    margin-left:50%;
    top:18vh;
    margin-top:-.25em;
    z-index: 600;
    font-style: normal; 
}
ul {
    position: relative;
    
    font-weight: lighter;
    font-family:'Lato', Helvetica, Arial, sans-serif;
		
    width: 100%;
    height: 100vh;
    text-align: right;
    list-style: none;
    width: 100%;
    height: 100vh;
    list-style: none;
}
.rightcol img {
    width: 95%;
    margin-bottom: 1em;
}
/* Nav Drawer */
body {
  overflow-x: hidden;
}

.container li {
    height:3em;
    
}
#toggle {
  display: none;
}

.social li {
    display:inline-block;
    width: 30px;
    margin-top:2em;
}

/**
  Hamburger
**/
.hamburger {
      position: fixed;
      top: 4.5em;
      transform: rotate(90deg);
      right: 22px;
      margin-top: -45px;
      width: 50px;
      height: 50px;
      z-index: 600;
}

.hamburger div {
  position: relative;
  width: 2.5em;
  height: 7px;
  border-radius: 3px;
  background-color: #12a7e0;
  margin-top: 8px;
  transition: all 0.3s ease-in-out;
}
video{
    width: 100%;
    max-width: 600px;
}
.smvid {
    margin: 0 auto;
   min-width: 40%;
    max-width: 50%;
    display: inline-block;
    float:left;
    clear:both;
    height: auto;

}
.slider {
    clear:both;
}

#toggle:checked + .hamburger .top-bun {
  transform: rotate(-45deg);
  margin-top: 22px;
    background-color:white;
}
#toggle:checked + .hamburger .bottom-bun {
  opacity: 0;
  transform: rotate(45deg);
    background-color:white;
}
#toggle:checked + .hamburger .meat {
  transform: rotate(45deg);
  margin-top: -7px;
    background-color:white;
}

#toggle:checked + .hamburger + .nav {
  top: 0;
  transform: scale(1);
}

/**
Nav Styles
**/
.nav {
  position: fixed;
  width: 100%;
  height: 100%;
  background-color: rgba(0,0,0,.92);
    margin-right:44px;
    top:0;
  overflow: hidden;
  transform: scale(0);
    z-index: 500;
    left:0;
    -webkit-transition:.75s ease-in-out;
    -moz-transition: .75s ease-in-out;
    -o-transition: .75s ease-in-out;
    transition: .75s ease-in-out;
    -webkit-transform: translate(9000x,0px);
    -moz-transform: translate(9000px,0px);
    -o-transform: translate(9000px,0px);
    transform: translate(9000px,0px)
}
.nav-wrapper {
      position: relative;
      overflow: hidden;
      overflow-y: auto;
      height: 100%;
    padding-right: 30px;
    top:0;
    text-align: right;  
}
nav {
  margin-left: 0;
}
nav a {
  position: relative;
  text-decoration: none;
  color: #fff;
font-size:1.6em;
    right:30px;
  display: inline-block;
  letter-spacing: 1px;
}

nav a:before {
  content: '';
  height: 0;
    
  position: absolute;
  width: 0.5em;
  left: -0.5em;
}
nav a:hover {
  color: #12a7e0;
}

.extra { 
    display: none;
}

a:hover .extra {
    display: inline-block;
	color:white;
}

nav a:hover:before {
  height: 100%;
}
.bg-content img {
    min-width: 100%;
    z-index: -100;
    align-content: center;
    background-position: center;
    vertical-align: middle;
    	top: 50%;
	left: 50%;
	-webkit-transform: translate(-50%, -50%);
	-moz-transform: translate(-50%, -50%);
	-ms-transform: translate(-50%, -50%);
	-o-transform: translate(-50%, -50%);
	transform: translate(-50%, -50%);
}
